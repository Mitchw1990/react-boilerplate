{
  "name": "react-boilerplate",
  "description": "Boilerplate for quickly developing performant React apps using Firebase, Parcel and the Ring UI component library.",
  "author": " Mitch Warrenburg",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [
    "boilerplate",
    "parcel",
    "react",
    "ring-ui",
    "Firebase"
  ],
  "license": "ISC",
  "engines": {
    "node": "9.11.2"
  },
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "jest": {
    "automock": true,
    "restoreMocks": true,
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^.*[.](less|svg|css|CSS)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/dev/jestSetup.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**"
    ],
    "coverageDirectory": "<rootDir>/coverage"
  },
  "dependencies": {
    "@jetbrains/ring-ui": "2.0.52",
    "axios": "^0.21.1",
    "firebase": "^7.2.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-firebase-hooks": "^2.1.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux-starter-kit": "^1.0.0",
    "trumbowyg": "^2.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.2",
    "@babel/runtime-corejs3": "^7.6.2",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.3.1",
    "@testing-library/react-hooks": "^3.1.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2"
  },
  "scripts": {
    "start": "parcel serve index.html",
    "test": "jest"
  }
}
